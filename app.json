{
  "expo": {
    "name": "hello-app",
    "slug": "hello-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/hello-app-logo-rounded.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/hello-app-logo-rounded.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.roycasso.helloapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/hello-app-logo-rounded.png"
    },
    "plugins": [
      "expo-router",
      "expo-build-properties"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "6ab7c996-69aa-4cfa-b78f-d92480f21853"
      }
    }
  }
}
